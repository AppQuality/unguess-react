{
  "name": "unguess-react",
  "version": "1.4.0",
  "private": true,
  "dependencies": {
    "@appquality/unguess-design-system": "3.0.0-colors.18",
    "@headwayapp/react-widget": "^0.0.4",
    "@reduxjs/toolkit": "^1.8.0",
    "@rtk-query/codegen-openapi": "^1.0.0-alpha.1",
    "@zendeskgarden/react-datepickers": "^8.49.4",
    "@zendeskgarden/react-dropdowns": "^8.49.0",
    "@zendeskgarden/react-forms": "^8.49.0",
    "@zendeskgarden/react-tables": "^8.62.0",
    "@zendeskgarden/react-theming": "^8.48.2",
    "@zendeskgarden/svg-icons": "^6.31.1",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "i18n-iso-countries": "^7.3.0",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.3",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-dropcursor": "^1.6.1",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-schema-list": "^1.2.2",
    "qs": "^6.10.3",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.15.7",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.7",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.3",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "react-scripts test",
    "recover": "yarn install --check-files",
    "eject": "react-scripts eject",
    "translate": "i18next 'src/**/*.{js,jsx,ts,tsx}' ",
    "generate-schema": ". ./.env.local; npx openapi-typescript $REACT_APP_API_URL/reference/ --output src/common/schema.ts ",
    "generate-api": ". ./.env.local; API_URL=$REACT_APP_API_URL/reference/ npx @rtk-query/codegen-openapi src/features/api/config.ts",
    "generate-strapi": "npx @rtk-query/codegen-openapi src/features/backoffice/config.ts",
    "type:check": "tsc",
    "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format:check": "yarn run prettier --check",
    "format:fix": "yarn run prettier --write",
    "lint": "eslint --ext .tsx,.ts .",
    "validate": "run-p lint type:check format:check",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "6.4.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-helmet": "^6.1.5",
    "@types/react-scroll": "^1.8.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-security": "^1.5.0",
    "husky": ">=6",
    "i18next-parser": "^7.0.3",
    "intl-pluralrules": "^1.3.1",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^5.1.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "lint-staged": {
    "*.{tsx,ts,js,css,md}": "prettier --write"
  }
}
